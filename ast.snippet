<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2010/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Assert.IsTrue</Title>
      <Description>指定された条件が true であることを検証します。</Description>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Shortcut>ast</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>condition</ID>
          <ToolTip>検証する条件</ToolTip>
          <Default>condition</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp" Kind="method body">
        <![CDATA[Assert.IsTrue($condition$);]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
